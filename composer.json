{
    "name": "codecovevienna/larafirebase",
    "description": "Laravel Firebase Cloud Messaging.",
    "homepage": "https://github.com/codecovevienna/larafirebase",
    "license": "MIT",
    "authors": [
        {
            "name": "Codecove Vienna",
            "email": "tools@codecove.at"
        }
    ],
    "autoload": {
        "psr-4": {
            "CodeCoveVienna\\Larafirebase\\": "src/"
        },
        "files" : [
            "src/Helpers/All.php"
        ]
    },
    "require": {
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/notifications": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "google/apiclient": "^2.12"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "CodeCoveVienna\\Larafirebase\\Providers\\LarafirebaseServiceProvider"
            ]
        }
    }
}
